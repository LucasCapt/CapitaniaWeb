      <commandText value="INSERT INTO dbo.Log4NetLog ([Date],[Thread],[Level],[Logger],           [Message],[Exception]) VALUES (@log_date, @thread, @log_level,            @logger, @message, @exception)" />
      <parameter>
        <parameterName value="@log_date" />
